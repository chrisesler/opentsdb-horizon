{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "frontend"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "../server/public",
      "assets": [
        "assets",
        "favicon.ico",
        {
          "glob": "**/*", 
          "input": "../node_modules/@ibm/plex/IBM-Plex-Mono/fonts/complete/woff", 
          "output": "./assets/fonts/IBM-Plex/mono" 
        },
        {
          "glob": "**/*", 
          "input": "../node_modules/@ibm/plex/IBM-Plex-Mono/fonts/complete/woff2", 
          "output": "./assets/fonts/IBM-Plex/mono" 
        },
        {
          "glob": "**/*", 
          "input": "../node_modules/@ibm/plex/IBM-Plex-Sans/fonts/complete/woff", 
          "output": "./assets/fonts/IBM-Plex/sans" 
        },
        {
          "glob": "**/*", 
          "input": "../node_modules/@ibm/plex/IBM-Plex-Sans/fonts/complete/woff2", 
          "output": "./assets/fonts/IBM-Plex/sans" 
        },
        {
          "glob": "**/*", 
          "input": "../node_modules/typeface-ubuntu-mono/files", 
          "output": "./assets/fonts/Ubuntu/mono" 
        },
        {
          "glob": "**/*", 
          "input": "../node_modules/typeface-ubuntu/files", 
          "output": "./assets/fonts/Ubuntu/sans" 
        },
        {
          "glob": "**/*", 
          "input": "../node_modules/typeface-roboto-mono/files", 
          "output": "./assets/fonts/Roboto/mono" 
        },
        {
          "glob": "**/*", 
          "input": "../node_modules/typeface-roboto/files", 
          "output": "./assets/fonts/Roboto/sans" 
        }
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "stylePreprocessorOptions": {
        "includePaths": [
          "."
        ]
      },
      "styles": [
        "debug.scss",
        "styles.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      },
      "serviceWorker": true
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
